'++LotusScript Development Environment:2:5:(Forward):0:1
Declare Function ViewQueryPaste(coll As NotesDocumentCollection) As Boolean

'++LotusScript Development Environment:2:5:(Declarations):0:2

'++LotusScript Development Environment:2:1:ViewQueryPaste:1:8
Function ViewQueryPaste(coll As NotesDocumentCollection) As Boolean
	
	On Error GoTo ERROR_
	
	Dim s As New NotesSession
	Dim doc As NotesDocument
	Dim pasted As NotesDocument
	
	Set doc = coll.GetFirstDocument
	
	Do Until (doc Is Nothing)
		Set pasted = s.CurrentDatabase.CreateDocument
		
		Call doc.CopyAllItems(pasted)
		
		If (pasted.HasItem("unique")) Then
			Call pasted.ReplaceItemValue("unique", Evaluate({@Unique}))
		End If
		
		Call pasted.Save(True, False)
		
		Set doc = coll.GetNextDocument(doc)
	Loop
	
EXIT_:
	Exit Function
	
ERROR_:
	Resume EXIT_
	
End Function